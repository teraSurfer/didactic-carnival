<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.tm.spring.beans.Employee" table="SPR_EMP">
    <id column="EID" name="employeeId">
      <generator class="assigned"/>
    </id>
    <property column="NAME" name="employeeName"/>
    <property column="SAL" name="employeeSalary"/>
  </class>
</hibernate-mapping>
